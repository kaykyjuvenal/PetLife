<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PetLifeKaykyFlavioJuvenal"
        tools:targetApi="31">
        <activity
            android:name="br.edu.ifsp.scl.ads.petlife.ui.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.EditarIdaAoVeterinarioActivity" />
        <activity android:name=".ui.EditarIdaAoPetshopActivity" />
        <activity android:name=".ui.EditarIdaParaVacinaActivity" />
        <activity android:name=".ui.EditarPetActivity"/>
        <activity android:name=".ui.LigarAoConsultorioActivity"/>
        <activity android:name=".ui.AbrirSiteConsultorioActivity"/>
        <activity android:name=".ui.AdicionarPetActivity"/>

    </application>

</manifest>